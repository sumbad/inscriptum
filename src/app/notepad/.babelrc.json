{
  "extends": "../../../babel.config.js",
  "plugins": [
    [
      "babel-plugin-transform-jsx-to-tt",
      {
        "tag": "html",
        "import": {
          "module": "lit-html2",
          "export": "html"
        },
        "attributes": [
          {
            "preset": "lit-html"
          }
        ]
      }
    ]
  ]
}