{
  "name": "inscriptum",
  "description": "blog inscriptum",
  "version": "3.14.2",
  "author": "Den Sumbaev @sumbad",
  "private": true,
  "scripts": {
    "generate": "graphql-codegen",
    "prestart": "npm run generate",
    "predev": "npm run dev:h && npm run generate",
    "dev": "cross-env NODE_ENV=development webpack serve --progress --stats-children --stats-error-details",
    "dev:h": "docker-compose --project-directory . -f hasura/docker-compose.yml up -d",
    "dev:h-up": "docker-compose --project-directory . -f hasura/docker-compose.yml up --no-start",
    "dev:h-down": "docker-compose --project-directory . -f hasura/docker-compose.yml down -v",
    "dev:h-start": "docker-compose --project-directory . -f hasura/docker-compose.yml start",
    "dev:h-stop": "docker-compose --project-directory . -f hasura/docker-compose.yml stop",
    "build": "NODE_ENV=production webpack --progress  --stats=verbose",
    "deploy": "npm run build && node ./scripts/.ghpages",
    "start": "npm run build && node browser-sync.js",
    "report": "rm -rf build && mkdir build && NODE_ENV=production webpack --progress --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json"
  },
