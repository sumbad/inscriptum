{
  "name": "inscriptum",
  "description": "blog inscriptum",
  "version": "2.0.0",
  "author": "Den Sumbaev @sumbad",
  "private": true,
  "scripts": {
    "dev": "npm run dev:h && npm run dev:f",
    "dev:f": "cross-env NODE_ENV=development webpack-dev-server --progress",
    "dev:h": "docker-compose -f hasura/docker-compose.yml up -d",
    "dev:h-up": "docker-compose -f hasura/docker-compose.yml up --no-start",
    "dev:h-down": "docker-compose -f hasura/docker-compose.yml down -v",
    "dev:h-start": "docker-compose -f hasura/docker-compose.yml start",
    "dev:h-stop": "docker-compose -f hasura/docker-compose.yml stop",
    "build": "NODE_ENV=production webpack --progress --verbose --hide-modules",
    "deploy": "node .ghpages",
    "start": "node browser-sync.js",
    "report": "rm -rf build && mkdir build && NODE_ENV=production webpack --progress --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.29",
