    "@fortawesome/free-solid-svg-icons": "^5.13.1",
    "@webcomponents/custom-elements": "^1.4.1",
    "@webcomponents/shadycss": "^1.10.0",
    "@webcomponents/shadydom": "^1.7.3",
    "@webcomponents/template": "^1.4.2",
    "@webcomponents/webcomponentsjs": "^1.3.3",
    "abstract-element": "^4.8.0",
    "auth0-js": "^9.13.4",
    "autosize": "^4.0.2",
    "blueimp-load-image": "^2.31.0",
    "bourbon": "^5.0.0",
    "cloudinary-core": "^2.9.0",
    "graphql-request": "^2.0.0",
    "highlight.js": "^9.18.1",
    "hyperhtml": "^2.32.2",
    "lit-html": "^1.2.1",
    "page": "^1.11.6",
    "presentarium": "^0.0.5",
    "quill": "^1.3.7",
    "rxjs": "6.5.4",
    "velocity-animate": "^1.5.1",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
